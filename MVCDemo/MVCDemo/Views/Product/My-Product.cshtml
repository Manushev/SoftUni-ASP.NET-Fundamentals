@using MVCDemo.Models.Product;
@model IEnumerable<ProductViewModel>

@{
    ViewData["Title"] = "All Product";
}

<div class="card" ">
    <div class="card-header">All Product</div>
    <div class="card-body">
        @foreach (var product in Model)
        {
            <li>N.@product.Id: @product.Name - @product.Price lv.</li>
        }
    </div>
</div>
